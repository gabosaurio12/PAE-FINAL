    <Button layoutX="704.0" layoutY="360.0" mnemonicParsing="false" onAction="#btnCSV" prefHeight="26.0" prefWidth="80.0" text="CSV" />
    <Button layoutX="704.0" layoutY="403.0" mnemonicParsing="false" onAction="#btnXLSX" prefHeight="26.0" prefWidth="80.0" text="XLSX" />
     <Button layoutX="714.0" layoutY="107.0" onAction="#agregarAvion" prefWidth="80" text="Agregar" />
     <Button layoutX="714.0" layoutY="147.0" onAction="#modificarAvion" prefWidth="80" text="Modificar" />
     <Button layoutX="714.0" layoutY="186.0" onAction="#eliminarAvion" prefWidth="80" text="Eliminar" />

</AnchorPane>
